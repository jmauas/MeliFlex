<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Lector Meli Flex</title>
  <link rel="icon" type="image/ico" href="media/favicon.ico">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.3/font/bootstrap-icons.css">
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@700&display=swap" rel="stylesheet">
  <link rel='manifest' href='./manifest.webmanifest'>
  <link href="./css/style.css" rel="stylesheet" type="text/css">
</head>
<body id="body">
    <div class="ppalC">
        <div class="loader-container">
            <div class="loader"></div>
            <div class="loader2"></div>
        </div>
    </div>
    <header>
        <%- include('partials/header') %>
    </header>
    <main>
        <% if (ruta=='root') { %>
            <%-   include('pages/cargar') %>
        <% } else if (ruta=='repa') { %>
            <%-   include('pages/repartidor') %>
        <% } else if (ruta=='info') { %>
            <%-   include('pages/info') %>
        <% } else if (ruta=='info.cliente') { %>
            <%-   include('pages/info-cliente') %>
        <% } %>
    </main> 
  <!-- JavaScript Bundle with Popper -->
  <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
  <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
  <script type='text/javascript' src="./js/auxiliares.js"></script>
  <script type='text/javascript' src="./js/jsQR.js"></script>
  <script type="text/javascript" src="./js/QRius.js"></script>
  <% if (ruta=='info' || ruta=='info.cliente') { %>
      <script type='text/javascript' src="./appInfo.js"></script>
   <% } else { %>
      <script type='text/javascript' src="./app.js"></script>
  <% } %>
</body>
</html>
